#include <iostream> // Incluir la biblioteca iostream para usar cout
using namespace std; // usando espacio de nombres STD
int main() // Función principal main para retornar un entero
{ // Inicio
	int numero; // Declaración de la variable "numero" como Entero
	
	numero = 5 + 5; // Asignación de la suma de 5+5 a la Variable
	cout << numero; // Imprimir el valor de numero
	return 0; // Retorno del Cero para finalizar programa
} // Cierre
